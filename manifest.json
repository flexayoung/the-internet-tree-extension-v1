{
    "name": "Internet Tree Extension",
    "description": "Testing",
    "version": "1.1",
  
    "manifest_version": 2,

     "background": {
        "scripts": [
            "worker.js"
        ]
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {                   
            "42":  "img/leaf.jpg"    
          }
    },
    "permissions": [
        "storage",
        "webRequest",
        "contextMenus",
        "unlimitedStorage",
        "alarms",
        "<all_urls>"
    ]
}